{% extends 'base.html.twig' %}

{% block body %}
    <h1>Productcommande</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ productCommande.id }}</td>
            </tr>
            <tr>
                <th>Quantite</th>
                <td>{{ productCommande.quantite }}</td>
            </tr>
            <tr>
                <th>Product</th>
                <td>{{ productCommande.product }}</td>
            </tr>
            <tr>
                <th>Commande</th>
                <td>{{ productCommande.commande }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ productCommande.prix }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('productcommande_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('productcommande_edit', { 'id': productCommande.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
